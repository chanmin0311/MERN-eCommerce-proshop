{
    "name": "proshop",
    "version": "0.0.1",
    "description": "eCommerce application built with the MERN stack",
    "type": "module",
    "main": "server.js",
    "scripts": {
        "start": "node backend/server.js",
        "server": "nodemon backend/server.js",
        "client": "npm start --prefix frontend",
        "dev": "concurrently \"npm run server\" \"npm run client\"",
        "data:import": "node backend/seeder.js",
        "data:destroy": "node backend/seeder.js -d"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/chanmin0311/MERN-eCommerce-proshop.git"
    },
    "author": "Chanmin Park",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/chanmin0311/MERN-eCommerce-proshop/issues"
    },
    "homepage": "https://github.com/chanmin0311/MERN-eCommerce-proshop#readme",
    "dependencies": {
        "bcryptjs": "^3.0.3",
        "colors": "^1.4.0",
        "express": "^5.1.0",
        "mongoose": "^9.0.0"
    },
    "devDependencies": {
        "concurrently": "^9.2.1",
        "dotenv": "^17.2.3",
        "nodemon": "^3.1.11"
    }
}
